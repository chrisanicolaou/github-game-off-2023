; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Github Game Jam 2023"
run/main_scene="res://src/main/main.tscn"
config/icon="res://icon.png"

[autoload]

RootSceneSwitcher="*res://common/scene_management/root_scene_switcher.tscn"
AudioManager="*res://common/audio/audio_manager.tscn"
InteractionManager="*res://common/interaction/interaction_manager/interaction_manager.tscn"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
mouse_cursor/custom_image="res://src/ui/mouse_cursors/default_cursor.png"
mouse_cursor/custom_image_hotspot=Vector2( 15, 15 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

Theme=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://src/ui/themes/primary_theme.tres"
theme/custom_font="res://src/ui/fonts/pixelify_12.tres"

